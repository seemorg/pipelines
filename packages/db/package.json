{
  "name": "@usul/db",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "postinstall": "pnpm generate",
    "build": "tsup",
    "dev": "tsup --watch",
    "generate": "prisma generate",
    "migrate:dev": "pnpm with-env prisma migrate dev",
    "migrate:deploy": "pnpm with-env prisma migrate deploy",
    "push": "pnpm with-env prisma db push --skip-generate",
    "studio": "pnpm with-env prisma studio",
    "with-env": "dotenv -e ../../.env --",
    "clean": "rm -rf .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@t3-oss/env-core": "^0.10.1",
    "prisma-json-types-generator": "^3.0.4"
  },
  "devDependencies": {
    "@usul/eslint-config": "workspace:*",
    "@usul/prettier-config": "workspace:*",
    "@usul/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "prisma": "^5.17.0",
    "tsup": "^8.0.2",
    "typescript": "catalog:"
  },
  "prettier": "@usul/prettier-config"
}
