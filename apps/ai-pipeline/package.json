{
  "name": "ai-pipeline",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "pnpm with-env node dist/src/index.js",
    "dev": "pnpm with-env tsup-node --watch --onSuccess \"node dist/src/index.js\"",
    "build": "pnpm with-env tsup-node",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@azure/search-documents": "^12.1.0",
    "@bull-board/api": "^5.21.1",
    "@bull-board/hono": "^5.21.1",
    "@hono/node-server": "^1.12.0",
    "@openiti/markdown-parser": "^1.2.2",
    "@t3-oss/env-core": "^0.10.1",
    "@usul/db": "workspace:*",
    "@usul/utils": "workspace:*",
    "bullmq": "^5.10.3",
    "hono": "^4.5.1",
    "ioredis": "^5.4.1",
    "llamaindex": "^0.8.19",
    "slugify": "^1.6.6",
    "string-strip-html": "^13.4.8",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@usul/eslint-config": "workspace:*",
    "@usul/prettier-config": "workspace:*",
    "@usul/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.0.2",
    "tsx": "^3.12.7",
    "typescript": "catalog:"
  },
  "prettier": "@usul/prettier-config"
}